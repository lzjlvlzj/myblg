    "css",
    "js",
    "responsive",
    "front-end",
    "web",
    "wysiwyg",
    "editor"
  ],
  "homepage": "https://github.com/steveathon/bootstrap-wysiwyg",
  "main": [
    "bootstrap-wysiwyg.js",
    "bootstrap-wysiwyg.min.js"
  ],
  "ignore": [
    ".*",
    "external/",
    "index.css",
    "index.html",
    "CONTRIBUTING"
  ],
  "dependencies": {
    "jquery": "~2.1.4",
    "jquery.hotkeys": "https://github.com/jeresig/jquery.hotkeys.git#master",
    "fontawesome": "~4.3.0",
    "bootstrap": "~3.3.4",
    "google-code-prettify": "~1.0.4"
  }
